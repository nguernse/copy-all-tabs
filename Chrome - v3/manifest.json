{

  "manifest_version": 3,
  "name": "Copy All Tabs",
  "description": "Copy all your tabs, meow",
  "version": "1.0",

  "background": {
    "service_worker": "worker_script.js"
  },
  
  "permissions": [
    "scripting",
    "tabs",
    "activeTab",
    "contextMenus",
    "clipboardWrite"
  ],
  
  "icons": {
    "16": "icons/page-16.png",
    "32": "icons/page-32.png",
    "48": "icons/page-48.png"
  }
}
